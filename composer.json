{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "barryvdh/laravel-debugbar": "^3.14",
        "barryvdh/laravel-dompdf": "^3.0",
        "doctrine/dbal": "^4.2",
        "guzzlehttp/guzzle": "^7.9",
        "illuminate/queue": "*",
        "intervention/image-laravel": "^1.3",
        "kphoen/rulerz": "^0.21.1",
        "laravel/framework": "^11.9",
        "laravel/horizon": "^5.29",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.11",
        "laravel/tinker": "^2.9",
        "league/commonmark": "^2.5",
        "php-ai/php-ml": "^0.10.0",
        "predis/predis": "^2.2",
        "proengsoft/laravel-jsvalidation": "^4.9",
        "pusher/pusher-php-server": "^7.2",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-medialibrary": "^11.9",
        "spatie/laravel-permission": "^6.9",
        "spatie/laravel-responsecache": "^7.6",
        "spatie/laravel-schemaless-attributes": "^2.5",
        "stancl/tenancy": "^3.8",
        "symfony/dom-crawler": "^7.1",
        "symfony/workflow": "^7.1",
        "workerman/workerman": "^4.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Http/Helpers/helpers.php",
            "app/Http/Helpers/TemplateHelpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
